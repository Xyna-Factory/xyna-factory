<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" BaseTypeName="ConstraintV2" BaseTypePath="xfmg.tmf.validation" IsAbstract="false" Label="Constraint Validation Result V2" TypeName="ConstraintValidationResultV2" TypePath="xfmg.tmf.validation" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data Label="failure type" VariableName="failureType">
    <Meta>
      <Type>String</Type>
      <Documentation>possible values: RULE, CONDITION, SKIPPED
RULE: condition was true, rule evaluated to false
CONDITION: condition was false
SKIPPED: the condition of one of the dependencies of the constraint evaluated to false</Documentation>
    </Meta>
  </Data>
  <Data Label="Exception Details" ReferenceName="ExceptionDetails" ReferencePath="core.exception" VariableName="exceptionDetails"/>
</DataType>
