// validate OrderItemActionType.value of type String
try {
    var validator_value = new com.gip.xyna.openapi.StringTypeValidator();
    validator_value.setName("Value");
    validator_value.setValue(getValue());
    validator_value.setAllowableValues("add", "modify", "delete", "noChange");
    result.getErrorMessages().addAll(validator_value.checkValid());
} catch (Exception e) {
    result.getErrorMessages().add("Error: could not validate OrderItemActionType.value");
}

return result.getResult();
