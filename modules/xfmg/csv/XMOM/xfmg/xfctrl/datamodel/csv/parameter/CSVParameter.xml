<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" Label="CSV Parameter" TypeName="CSVParameter" TypePath="xfmg.xfctrl.datamodel.csv.parameter" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data Label="Header" VariableName="header">
    <Meta>
      <Documentation>Enumeration(none,labels,paths)
Default: none

none: no header
labels: xmom labels
paths: xfl like paths</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Masker" VariableName="masker">
    <Meta>
      <Documentation>Default "\""</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Separator" VariableName="separator">
    <Meta>
      <Documentation>Default ","</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="LineSeparator" VariableName="lineSeparator">
    <Meta>
      <Documentation>Default "\n"</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Null Representation" VariableName="nullRepresentation">
    <Meta>
      <Documentation>Default ""</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="End of File" VariableName="endOfFile">
    <Meta>
      <Documentation>Default ""</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
</DataType>
