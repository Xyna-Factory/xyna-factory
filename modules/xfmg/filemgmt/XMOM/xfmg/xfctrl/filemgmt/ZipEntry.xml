<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" IsAbstract="false" Label="Zip Entry" TypeName="ZipEntry" TypePath="xfmg.xfctrl.filemgmt" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data Label="name" VariableName="name">
    <Meta>
      <Type>String</Type>
      <Documentation>path and name of entry inside of zip file, separated by /</Documentation>
    </Meta>
  </Data>
  <Data Label="last modified" VariableName="lastModified">
    <Meta>
      <Type>long</Type>
      <Documentation>time stamp of last modification (ms since 1970). 0 if unknown</Documentation>
    </Meta>
  </Data>
  <Data Label="uncompressed size" VariableName="uncompressedSize">
    <Meta>
      <Type>long</Type>
      <Documentation>in bytes. 0 if entry is directory.</Documentation>
    </Meta>
  </Data>
  <Data Label="is directory" VariableName="isDirectory">
    <Meta>
      <Type>boolean</Type>
    </Meta>
  </Data>
</DataType>
