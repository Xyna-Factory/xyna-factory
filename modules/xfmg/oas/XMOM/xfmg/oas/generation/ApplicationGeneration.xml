--><DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" IsAbstract="false" Label="Application Generation" TypeName="ApplicationGeneration" TypePath="xfmg.oas.generation" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>true</IsServiceGroupOnly>
  </Meta>
  <Libraries>ApplicationGenerationImpl.jar</Libraries>
  <SharedLibraries>ZetaTools</SharedLibraries>
  <SharedLibraries>xyna-openapi</SharedLibraries>
  <Service Label="Application Generation" TypeName="ApplicationGeneration">
    <Meta>
      <AdditionalDependencies>
        <Datatype>xmcp.forms.plugin.PluginManagement</Datatype>
      </AdditionalDependencies>
    </Meta>
    <Operation IsStatic="true" Label="Generate Application" Name="generateApplication" RequiresXynaOrder="true">
      <Input>
        <Data ID="1" Label="Application Generation Parameter" ReferenceName="ApplicationGenerationParameter" ReferencePath="xfmg.oas.generation" VariableName="applicationGenerationParameter1"/>
        <Data ID="4" Label="File" ReferenceName="File" ReferencePath="base" VariableName="file4"/>
      </Input>
      <Output/>
      <SourceCode>
        <CodeSnippet Type="Java">xfmg.oas.generation.ApplicationGenerationImpl.generateApplication(correlatedXynaOrder, applicationGenerationParameter1, file4);</CodeSnippet>
      </SourceCode>
    </Operation>
    <Operation IsStatic="true" Label="Generate Application by managed File ID" Name="generateApplicationByManagedFileID" RequiresXynaOrder="true">
      <Input>
        <Data ID="2" Label="Application Generation Parameter" ReferenceName="ApplicationGenerationParameter" ReferencePath="xfmg.oas.generation" VariableName="applicationGenerationParameter2"/>
        <Data ID="3" Label="Managed File Id" ReferenceName="ManagedFileId" ReferencePath="xfmg.xfctrl.filemgmt" VariableName="managedFileId3"/>
      </Input>
      <Output/>
      <SourceCode>
        <CodeSnippet Type="Java">xfmg.oas.generation.ApplicationGenerationImpl.generateApplicationByManagedFileID(correlatedXynaOrder, applicationGenerationParameter2, managedFileId3);</CodeSnippet>
      </SourceCode>
    </Operation>
    <Operation IsStatic="true" Label="Query Oas Import History" Name="queryOasImportHistory">
      <Input>
        <Data ID="6" Label="Table Info" ReferenceName="TableInfo" ReferencePath="xmcp.tables.datatypes" VariableName="tableInfo6"/>
      </Input>
      <Output>
        <Data ID="5" IsList="true" Label="OAS_ImportHistory" ReferenceName="OAS_ImportHistory" ReferencePath="xmcp.oas.fman.storables" VariableName="oAS_ImportHistory5"/>
      </Output>
      <SourceCode>
        <CodeSnippet Type="Java">return xfmg.oas.generation.ApplicationGenerationImpl.queryOasImportHistory(tableInfo6);</CodeSnippet>
      </SourceCode>
    </Operation>
    <Operation IsStatic="true" Label="Store Oas Import History" Name="storeOasImportHistory">
      <Input>
        <Data ID="7" Label="OAS_ImportHistory" ReferenceName="OAS_ImportHistory" ReferencePath="xmcp.oas.fman.storables" VariableName="oAS_ImportHistory7"/>
      </Input>
      <Output/>
      <SourceCode>
        <CodeSnippet Type="Java">xfmg.oas.generation.ApplicationGenerationImpl.storeOasImportHistory(oAS_ImportHistory7);</CodeSnippet>
      </SourceCode>
    </Operation>
    <Operation IsStatic="true" Label="Query Oas Import History Details" Name="queryOasImportHistoryDetails">
      <Input>
        <Data ID="8" Label="OAS_ImportHistory" ReferenceName="OAS_ImportHistory" ReferencePath="xmcp.oas.fman.storables" VariableName="oAS_ImportHistory8"/>
      </Input>
      <Output>
        <Data ID="9" Label="OAS_ImportHistory" ReferenceName="OAS_ImportHistory" ReferencePath="xmcp.oas.fman.storables" VariableName="oAS_ImportHistory9"/>
      </Output>
      <SourceCode>
        <CodeSnippet Type="Java">return xfmg.oas.generation.ApplicationGenerationImpl.queryOasImportHistoryDetails(oAS_ImportHistory8);</CodeSnippet>
      </SourceCode>
    </Operation>
  </Service>
</DataType>
