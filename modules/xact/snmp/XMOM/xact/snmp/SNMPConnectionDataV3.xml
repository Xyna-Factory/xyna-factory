<DataType BaseTypeName="SNMPConnectionData" BaseTypePath="xact.snmp" Label="SNMPConnectionDataV3" TypeName="SNMPConnectionDataV3" TypePath="xact.snmp" Version="1.8" xmlns="http://www.gip.com/xyna/xdev/xfractmod">
  <Data Label="User Name" VariableName="userName">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Auth Algo" ReferenceName="SNMPv3AuthAlgorithm" ReferencePath="xact.snmp.algorithms" VariableName="sNMPv3AuthAlgorithm">
    <Meta>
      <Documentation>if not set explicitly use value of Xyna Property xact.snmp.v3.authalgorithm</Documentation>
    </Meta>
  </Data>
  <Data Label="Priv Algo" ReferenceName="SNMPv3PrivAlgorithm" ReferencePath="xact.snmp.algorithms" VariableName="sNMPv3PrivAlgorithm">
    <Meta>
      <Documentation>if not set explicitly use value of Xyna Property xact.snmp.v3.privalgorithm</Documentation>
    </Meta>
  </Data>
</DataType>
