<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" Label="LDAP Extended Request" TypeName="LDAPExtendedRequest" TypePath="xact.ldap" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
    <Documentation>Encapsulates an ID which uniquely identifies a particular extended operation, known to a particular server, and the data associated with that extended operation.
The requestValue will be converted to bytes as encoded by the utf8 charset before usage.</Documentation>
  </Meta>
  <Data Label="request name" VariableName="requestName">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="request value" VariableName="requestValue">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
</DataType>
