 <XynaCommandLineCommand>
  <CommandDefinition Name="generatefromldapschema" Groups="LDAP Management" Description="Generates datatypes from the ldap schema definition of a host."/>
  <Argument Name="host" Description="Host name of the LDAP server."/>
  <Argument Name="port" Description="Port of the LDAP server." Optional="true"/>
  <Argument Name="username" Description="Username for schema download if required." Optional="true"/>
  <Argument Name="password" Description="Password for schema download if required." Optional="true"/>
</XynaCommandLineCommand>
