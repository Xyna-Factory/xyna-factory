<Application applicationName="SOAP" comment="" factoryVersion="" versionName="1.0.5" xmlVersion="1.1">
  <ApplicationInfo>
    <Description Lang="DE">Tools zur Benutzung des SOAP-Protokolls</Description>
    <Description Lang="EN">Tools for using the SOAP protocol</Description>
    <RuntimeContextRequirements>
      <RuntimeContextRequirement>
        <ApplicationName>Base</ApplicationName>
        <VersionName>1.1.3</VersionName>
      </RuntimeContextRequirement>
    </RuntimeContextRequirements>
  </ApplicationInfo>
  <SharedLibs>
    <SharedLib implicitDependency="true">
      <SharedLib>soaplib</SharedLib>
    </SharedLib>
  </SharedLibs>
  <XMOMEntries>
    <XMOMEntry implicitDependency="false">
      <FqName>xact.soap.SOAPServices</FqName>
      <Type>DATATYPE</Type>
    </XMOMEntry>
    <XMOMEntry implicitDependency="false">
      <FqName>xact.soap.WebserviceResponseBody</FqName>
      <Type>DATATYPE</Type>
    </XMOMEntry>
    <XMOMEntry implicitDependency="false">
      <FqName>xact.soap.HeaderField</FqName>
      <Type>DATATYPE</Type>
    </XMOMEntry>
    <XMOMEntry implicitDependency="false">
      <FqName>xact.soap.WebserviceConnectInformation</FqName>
      <Type>DATATYPE</Type>
    </XMOMEntry>
    <XMOMEntry implicitDependency="false">
      <FqName>xact.soap.WebserviceConnectString</FqName>
      <Type>DATATYPE</Type>
    </XMOMEntry>
    <XMOMEntry implicitDependency="false">
      <FqName>xact.soap.WebserviceEndpoint</FqName>
      <Type>DATATYPE</Type>
    </XMOMEntry>
  </XMOMEntries>
</Application>
