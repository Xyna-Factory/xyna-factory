<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" Label="SSH Server Parameter" TypeName="SSHServerParameter" TypePath="xact.ssh.file" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data Label="Host" VariableName="host">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="User" VariableName="user">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Password" VariableName="password">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Port" VariableName="port">
    <Meta>
      <Type>Integer</Type>
    </Meta>
  </Data>
  <Data Label="PrivateKey" VariableName="privateKey">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="PublicKey" VariableName="publicKey">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="PassPhrase" VariableName="passPhrase">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="PrivateKeyFile" VariableName="privateKeyFile">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="KnownHostFile" VariableName="knownHostFile">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Bandwidth" VariableName="bandwidth">
    <Meta>
      <Documentation>Limit Bandwidth;  kbit/s</Documentation>
      <Type>Integer</Type>
    </Meta>
  </Data>
  <Data Label="SCPTimeouts" ReferenceName="SCPTimeouts" ReferencePath="xact.ssh.file" VariableName="sCPTimeouts"/>
</DataType>
