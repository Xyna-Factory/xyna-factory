--><DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" IsAbstract="false" Label="Access Response Parameter" TypeName="AccessResponseParameter" TypePath="xact.radius" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
    <Documentation>This data type is passed to the filter to build the Access-Accept or Access-Reject message.</Documentation>
  </Meta>
  <Data Label="Code" ReferenceName="Code" ReferencePath="xact.radius" VariableName="code">
    <Meta>
      <Documentation>RADIUS Code</Documentation>
    </Meta>
  </Data>
  <Data Label="Identifier" ReferenceName="Identifier" ReferencePath="xact.radius" VariableName="identifier">
    <Meta>
      <Documentation>The request identifier from the Access-Request.</Documentation>
    </Meta>
  </Data>
  <Data Label="Request Authenticator" ReferenceName="RequestAuthenticator" ReferencePath="xact.radius" VariableName="requestAuthenticator">
    <Meta>
      <Documentation>The Request Authenticator from the Access-Request</Documentation>
    </Meta>
  </Data>
  <Data IsList="true" Label="Node" ReferenceName="Node" ReferencePath="xact.radius" VariableName="node">
    <Meta>
      <Documentation>List of RADIUS attributes.</Documentation>
    </Meta>
  </Data>
  <Data Label="Shared Secret" VariableName="sharedSecret">
    <Meta>
      <Type>String</Type>
      <Documentation>Encrypted user-specific shared secret. If null or empty, the filter uses the default shared secret from the property xact.radius.sharedSecret.</Documentation>
    </Meta>
  </Data>
</DataType>
