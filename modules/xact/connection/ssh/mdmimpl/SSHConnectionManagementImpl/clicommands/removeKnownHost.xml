 <XynaCommandLineCommand>
  <CommandDefinition Name="removeknownhost" Groups="SSH Connection Management" Description="Remove the entries for a registered host."/>
  <Argument Name="hostname" Description="Hostname who's entries should be removed."/>
  <Argument Name="publickey" Description="Specific key to remove." Optional="true"/>
  <Argument Name="encryptiontype" Description="The encryption type (one of: RSA, DSA) of the entries to be removed." Optional="true"/>
</XynaCommandLineCommand>
