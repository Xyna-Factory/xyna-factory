    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data Label="type" ReferenceName="EncryptionAlgorithmType" ReferencePath="xact.ssh" VariableName="type"/>
  <Data Label="key size" VariableName="keySize">
