 --><DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" Version="1.8" TypeName="IPv4Subnet" TypePath="test.xypilot" Label="IPv4 Subnet" IsAbstract="false">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data Label="address" VariableName="address" ReferenceName="IPv4Address" ReferencePath="test.xypilot">
    <Meta>
      <Documentation>The ip address of the subnet.</Documentation>
    </Meta>
  </Data>
  <Data Label="mask" VariableName="mask">
    <Meta>
      <Type>int</Type>
      <Documentation>The number of bits that are fixed in the IPv4 address. 
The value must be between 0 and 32. 
A value of 0 means that the address is not fixed at all, 
a value of 32 means that the address is completely fixed.</Documentation>
    </Meta>
  </Data>
</DataType>
