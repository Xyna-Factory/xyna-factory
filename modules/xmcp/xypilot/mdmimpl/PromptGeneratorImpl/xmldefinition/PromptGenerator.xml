 --><DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" IsAbstract="false" Label="PromptGenerator" TypeName="PromptGenerator" TypePath="xmcp.xypilot" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>true</IsServiceGroupOnly>
  </Meta>
  <Libraries>PromptGeneratorImpl.jar</Libraries>
  <Service Label="PromptGenerator" TypeName="PromptGenerator">
    <Operation IsStatic="true" Label="Tests" Name="tests">
      <Input>
        <Data ID="1" Label="XMOM Item Reference" ReferenceName="XMOMItemReference" ReferencePath="xmcp.xypilot" VariableName="xMOMItemReference1"/>
      </Input>
      <Output>
        <Data ID="2" Label="Text" ReferenceName="Text" ReferencePath="base" VariableName="text2"/>
      </Output>
      <SourceCode>
        <CodeSnippet Type="Java">return PromptGeneratorImpl.tests(xMOMItemReference1);</CodeSnippet>
      </SourceCode>
    </Operation>
    <Operation IsStatic="true" Label="generate Type Documentation" Name="generateTypeDocumentation">
      <Input>
        <Data ID="3" Label="XMOM Item Reference" ReferenceName="XMOMItemReference" ReferencePath="xmcp.xypilot" VariableName="xMOMItemReference3"/>
      </Input>
      <Output>
        <Data ID="4" Label="Text" ReferenceName="Text" ReferencePath="base" VariableName="text4"/>
      </Output>
      <Meta>
        <Documentation>Generates Documentation for the DataType described by the XMOM Item Reference.

If anything doesn't work, an empty Text is returned.</Documentation>
      </Meta>
      <SourceCode>
        <CodeSnippet Type="Java">return PromptGeneratorImpl.generateTypeDocumentation(xMOMItemReference3);</CodeSnippet>
      </SourceCode>
    </Operation>
  </Service>
</DataType>
