 --><DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" IsAbstract="false" Label="Parameter" TypeName="Parameter" TypePath="xmcp.xypilot" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
    <Documentation>A parameter (e.g. of a method) to which to bind a variable.</Documentation>
  </Meta>
  <Data Label="name" VariableName="name">
    <Meta>
      <Type>String</Type>
      <Documentation>Name of the parameter. This is the name that the parameter is referred to in generated code.</Documentation>
    </Meta>
  </Data>
  <Data Label="type" VariableName="type">
    <Meta>
      <Type>String</Type>
      <Documentation>The type of the parameter. Must be a reference type.</Documentation>
    </Meta>
  </Data>
  <Data Label="isList" VariableName="isList">
    <Meta>
      <Type>boolean</Type>
      <Documentation>Whether this parameter is a list of &lt;type&gt;</Documentation>
    </Meta>
  </Data>
</DataType>
