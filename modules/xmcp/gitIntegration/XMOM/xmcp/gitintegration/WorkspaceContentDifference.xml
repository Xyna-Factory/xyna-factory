<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" IsAbstract="false" Label="Workspace Content Difference" TypeName="WorkspaceContentDifference" TypePath="xmcp.gitintegration" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data Label="Id" VariableName="id">
    <Meta>
      <Type>long</Type>
    </Meta>
  </Data>
  <Data Label="Content Type" VariableName="contentType">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Difference Type" ReferenceName="WorkspaceContentDifferenceType" ReferencePath="xmcp.gitintegration" VariableName="differenceType"/>
  <Data Label="Existing Item" ReferenceName="WorkspaceContentItem" ReferencePath="xmcp.gitintegration" VariableName="existingItem">
    <Meta>
      <Documentation>Is null when DifferenceType is CREATE</Documentation>
    </Meta>
  </Data>
  <Data Label="New Item" ReferenceName="WorkspaceContentItem" ReferencePath="xmcp.gitintegration" VariableName="newItem">
    <Meta>
      <Documentation>Is null when Difference Type is DELETE</Documentation>
    </Meta>
  </Data>
</DataType>
