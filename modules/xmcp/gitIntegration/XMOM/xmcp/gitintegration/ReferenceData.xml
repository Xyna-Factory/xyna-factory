--><DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" IsAbstract="false" Label="Reference Data" TypeName="ReferenceData" TypePath="xmcp.gitintegration" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data Label="Object Name" VariableName="objectName">
    <Meta>
      <Type>String</Type>
      <Documentation>FqName of the Object using this reference</Documentation>
    </Meta>
  </Data>
  <Data Label="Object Type" VariableName="objectType">
    <Meta>
      <Type>String</Type>
      <Documentation>Type of the Object for which this reference is relevant: DATATYPE, TRIGGER, FILTER</Documentation>
    </Meta>
  </Data>
  <Data Label="Path" VariableName="path">
    <Meta>
      <Type>String</Type>
      <Documentation>Path to where the referenced files are stored/generated from</Documentation>
    </Meta>
  </Data>
  <Data Label="Reference Type" VariableName="referenceType">
    <Meta>
      <Type>String</Type>
      <Documentation>Type of the reference: build (ant build will be called here), lib_folder (jar files under this folder are considered for build support</Documentation>
    </Meta>
  </Data>
  <Data Label="Workspace Name" VariableName="workspaceName">
    <Meta>
      <Type>String</Type>
      <Documentation>Workspace in which the object is registered</Documentation>
    </Meta>
  </Data>
</DataType>
