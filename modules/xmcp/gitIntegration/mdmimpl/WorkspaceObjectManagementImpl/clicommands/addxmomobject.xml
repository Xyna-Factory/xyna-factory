<XynaCommandLineCommand>
  <CommandDefinition Name="addxmomobject" Description="Registers the XMOM object with xml at the specified path. If the deploy flag is set, it will then be deployed." />
  <Argument Name="workspaceName" Description="Workspace in which the XMOM object is registered" />
  <Argument Name="fqName" Description="fqName of the XMOM object" />
  <BoolOption Name="d" LongName="deploy" Description="Deploy the XMOM object" Optional="true"/>
</XynaCommandLineCommand>