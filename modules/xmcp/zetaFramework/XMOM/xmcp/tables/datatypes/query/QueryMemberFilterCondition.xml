<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" Label="Query Member Filter Condition" TypeName="QueryMemberFilterCondition" TypePath="xmcp.tables.datatypes.query" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data Label="path" VariableName="path">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="value" VariableName="value">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="operator" VariableName="operator">
    <Meta>
      <Type>String</Type>
      <Documentation>Folgende Operatoren werden unterstützt:
- Gleichheit: {"=", "==",  "EQUALS"}
- Ungleichheit: {"!=", "!==", "NOT EQUALS"}
- Enthält: {"CONTAINS"}</Documentation>
    </Meta>
  </Data>
</DataType>
