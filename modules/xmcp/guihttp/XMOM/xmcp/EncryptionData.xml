 --><DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" IsAbstract="false" Label="Encryption Data" TypeName="EncryptionData" TypePath="xmcp" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Data IsList="true" Label="Values" VariableName="values">
    <Meta>
      <Type>String</Type>
      <Documentation>List of encrypted values or values to encrypt, depending on the state of the "Encrypted" flag.</Documentation>
