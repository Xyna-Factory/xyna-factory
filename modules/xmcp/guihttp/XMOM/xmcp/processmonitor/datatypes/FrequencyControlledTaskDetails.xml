  <Data Label="Event count" VariableName="eventCount">
    <Meta>
      <Type>long</Type>
    </Meta>
  </Data>
  <Data Label="Failed events" VariableName="failedEvents">
    <Meta>
      <Type>long</Type>
    </Meta>
  </Data>
  <Data Label="Finished events" VariableName="finishedEvents">
    <Meta>
      <Type>long</Type>
    </Meta>
