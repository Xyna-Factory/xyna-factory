<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" BaseTypeName="RuntimeContext" BaseTypePath="xmcp" Label="Workspace" TypeName="Workspace" TypePath="xmcp" Version="1.8">
  <Meta>
    <IsServiceGroupOnly>false</IsServiceGroupOnly>
  </Meta>
  <Service Label="Workspace" TypeName="Workspace">
    <Operation IsStatic="false" Label="getGUIRepresentation" Name="getGUIRepresentation">
      <Meta>
        <FixedDetailOptions>openDocumentation</FixedDetailOptions>
      </Meta>
      <Input/>
      <Output>
        <Data Label="Text" VariableName="text">
          <Meta>
            <Type>String</Type>
          </Meta>
        </Data>
      </Output>
      <SourceCode>
        <CodeSnippet Type="Java">Workspace workspace = this;

if(workspace == null || workspace.getName() == null) 
  return "";

return "WS:\"" + workspace.getName() + "\"";</CodeSnippet>
      </SourceCode>
    </Operation>
  </Service>
</DataType>
