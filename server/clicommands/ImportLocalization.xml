<XynaCommandLineCommand>
  <CommandDefinition Name="importlocalization" Groups="Misc" Description="Import an a file with localization data."/>
  <Argument Name="filename" Description="The name of the xml file containing the localization data. Supported format: XML PersistenceLayer XML-File."/>
  <BoolOption Name="o" LongName="overwriteExistingData" Description="Overwrite existing localization data." Optional="true"/>
</XynaCommandLineCommand>
