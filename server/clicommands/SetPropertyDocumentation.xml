  <CommandDefinition Dependencies="NONE" Name="setpropertydocumentation" Groups="Xyna Properties" Description="Sets documentation of a property."/>
  <Argument Name="key" Description="The name of the property to be set"/>
  <Argument Name="language" Description="Valid languages are: EN, DE. Default is EN" Optional="true"/>  
