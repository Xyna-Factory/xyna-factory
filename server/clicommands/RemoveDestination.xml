<XynaCommandLineCommand>
  <CommandDefinition Name="removedestination" Groups="Dispatcher Management" Description="Removes a destination from the specified dispatcher."/>
  <Argument Name="dispatcherName" Description="Valid dispatcherNames are: Planning, Execution and Cleanup"/>
  <Argument Name="destinationKey" Description="The destination key to be removed"/>
  <Argument Name="applicationName" Description="The name of the application." Optional="true"/>
  <Argument Name="versionName" Description="The name of the version." Optional="true"/>
  <Argument Name="workspaceName" Description="The name of the workspace." Optional="true"/>
</XynaCommandLineCommand>
