<XynaCommandLineCommand>
  <CommandDefinition Name="cancelorder" Groups="Misc" Description="Cancel the order with the provided ID."/>
  <Argument Name="orderId" Description="The ID of the order to be canceled"/>
  <Argument Name="timeout" Description="The timeout in seconds after which the request will fail. If no value is specified, a default of 5 seconds will be used" Optional="true"/>
</XynaCommandLineCommand>
