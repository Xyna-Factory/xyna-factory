<XynaCommandLineCommand>
  <CommandDefinition Name="modifydatamodel" Groups="Data Model Management" Description="Modifies an existing data model."/>
  <Argument Name="datamodeltype" Description="The name of the data model type"/>
  <Argument Name="datamodelname" Description="The name of the data model"/>
  <Argument Name="datamodelversion" Description="The version of the data model"/>
  <Argument Name="parameters" Description="The parameters for the data model or 'help' to show possible parameters" MultipleValues="true" Optional="true"/>
