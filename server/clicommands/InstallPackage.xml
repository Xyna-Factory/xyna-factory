<XynaCommandLineCommand>
  <CommandDefinition Name="installpackage" Groups="Backup and Restore" Description="Install a previously created package from a zip file."/>
  <Argument Name="sourceZipFile" Description="The path to the zip file containing the package to be installed"/>
  <BoolOption Name="f" LongName="forceoverwrite" Description="Overwrite possibly existing files"/>
  <BoolOption Name="v" LongName="verbose" Description="Enable verbose output"/>
  <BoolOption Name="noupdate" LongName="noupdatemdm" Description="disables the automatic attempt to update outdated mdm files"/>
</XynaCommandLineCommand>
