<XynaCommandLineCommand>
  <CommandDefinition Name="processmanualinteraction" Groups="Manual Interaction" Description="Process a pending manual interaction."/>
  <Argument Name="id" Description="The ID of the manual interaction to process"/>
  <Argument Name="response" Description="The response that is sent to the corresponding order.  Valid values for &lt;response&gt; are 'Abort', 'Retry' and 'Continue'."/>
</XynaCommandLineCommand>
