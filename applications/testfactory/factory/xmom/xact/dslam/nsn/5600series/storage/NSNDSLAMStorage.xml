<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" Label="NSN DSLAM Storage" TypeName="NSNDSLAMStorage" TypePath="xact.dslam.nsn.5600series.storage" Version="1.8">
  <Meta>
