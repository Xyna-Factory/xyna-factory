<ExceptionStore xmlns="http://www.gip.com/xyna/3.0/utils/message/storage/1.1"
              Name="ExceptionStore" Version="1.0">
  <Description>Exceptions. Use "ant genCodeClass" to generate corresponding Java classes.</Description>
  <!-- This import gives the possibility to inherit from server exceptions... -->
  <Import File="${BLACK_SERVER_HOME}/Exceptions.xml" />
  <!--  Example  -->
  
   
  <ExceptionType Code="XYNA-09690" TypeName="SFTPTRIGGER_COULD_NOT_BE_STARTED" TypePath="com.gip.xyna.xact.triggers"  BaseTypeName="XACT_TriggerCouldNotBeStartedException" BaseTypePath="com.gip.xyna.xact.exceptions">
    <MessageText Language="EN">exception during startup of ssh server: </MessageText>
    <MessageText Language="DE">Fehler beim Starten des ssh Servers: </MessageText>
  </ExceptionType>
   
</ExceptionStore>
