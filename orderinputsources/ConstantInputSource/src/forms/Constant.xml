<DataType xmlns="http://www.gip.com/xyna/xdev/xfractmod" Label="Constant" 
TypePath="xmcp.factorymanager.orderinputsourcetype" TypeName="Constant">
  <Data Label="Input Data" VariableName="inputData">
    <Meta>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Priority" VariableName="priority">
    <Meta>
		<Documentation>Priority of order (highest = 10, lowest = 1)
		</Documentation>
		<Type>Integer</Type>
    </Meta>
    <Restriction>
      <DefaultValue>7</DefaultValue>
      <MinValue>1</MinValue>
      <StepValue>1</StepValue>
      <MaxValue>10</MaxValue>
    </Restriction> 
  </Data>
  <Data Label="Custom Field 0" VariableName="customField0">
    <Meta>
      <Documentation>Custom Field 0. Some userdefined value to be used to recognize orders.</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Custom Field 1" VariableName="customField1">
    <Meta>
      <Documentation>Custom Field 1. Some userdefined value to be used to recognize orders.</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Custom Field 2" VariableName="customField2">
    <Meta>
      <Documentation>Custom Field 2. Some userdefined value to be used to recognize orders.</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Custom Field 3" VariableName="customField3">
    <Meta>
      <Documentation>Custom Field 3. Some userdefined value to be used to recognize orders.</Documentation>
      <Type>String</Type>
    </Meta>
  </Data>
  <Data Label="Monitoring Level" VariableName="monitoringLevel">
    <Meta>
      <Documentation>Monitoring Level of order (0, 5, 10, 15, 17, 18 or 20)</Documentation>
      <Type>Integer</Type>
    </Meta>
    <Restriction>
      <DefaultValue>20</DefaultValue>
      <PossibleValue>0</PossibleValue>
      <PossibleValue>5</PossibleValue>
      <PossibleValue>10</PossibleValue>
      <PossibleValue>15</PossibleValue>
      <PossibleValue>17</PossibleValue>
      <PossibleValue>18</PossibleValue>
      <PossibleValue>20</PossibleValue>
    </Restriction>
  </Data>
</DataType>
