<schema xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:jms="http://xmlns.oracle.com/JMS_Headers"
	targetNamespace="http://www.gip.com/xyna/1.5/xsd/faults/1.0"
	xmlns:xpmcom="http://www.gip.com/xyna/1.5/xsd/faults/1.0"
	elementFormDefault="qualified">
	<complexType name="XynaFault_ctype">
		<sequence>
			<element name="Code" type="string" />
			<element name="Summary" type="string" />
			<element name="Details" type="string" />
		</sequence>
	</complexType>
	<element name="XynaFault" type="xpmcom:XynaFault_ctype" />
</schema>